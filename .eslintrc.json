{
    "env": {
        "node": true,
        "jest": true,
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/extensions": "off",
        "no-console": "off",
        "no-undef": "off",
        "react/prop-types": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js",   ".jsx"] }]
    }
}
